{
  "name": "taskflow-reactive",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4202",
    "start:clean": "npm run clean:local && ng serve --port 4202",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rm -rf dist .angular/cache node_modules package-lock.json && npm cache clean --force && ng cache clean && pnpm install",
