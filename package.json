    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.22.4",
    "primeicons": "^6.0.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^19.0.3",
